<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="engtelecom.poo.luiza.ifnightfood"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- acessando a internet -->
    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="engtelecom.poo.luiza.ifnightfood.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="engtelecom.poo.luiza.ifnightfood.PrincipalActivity"
            android:parentActivityName="engtelecom.poo.luiza.ifnightfood.LoginActivity" />
        <activity
            android:name="engtelecom.poo.luiza.ifnightfood.ReservasActivity"
            android:parentActivityName="engtelecom.poo.luiza.ifnightfood.PrincipalActivity" />
        <activity
            android:name="engtelecom.poo.luiza.ifnightfood.CardapioActivity"
            android:parentActivityName="engtelecom.poo.luiza.ifnightfood.PrincipalActivity" />
    </application>

</manifest>